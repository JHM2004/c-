#include<iostream>
#include<iomanip>
using namespace std;
int main()
{	
	int i = 0;
	int ret = 0;
	for (i = 1; i <= 100; i++)
	{
		if (i % 3!=0)
		{
			continue;
		}
		else
		{
			cout <<setw(3)<< i << ' ';
			if (++ret%10 == 0)
			{
				cout << endl;
			}
		}
	}
	return 0;
}

