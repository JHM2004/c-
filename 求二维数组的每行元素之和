#include<iostream>
using namespace std;
int main()
{
	double arr[][4] = { {4.4,5.5,6.6,7.7},{3.3,324.0,324.5,43.4},{2.3,4.5,65.5,564.54} };
	int i = 0;
	for (i = 0; i < 3; i++)
	{
		int j = 0;
		double sum = 0;
		for (j = 0; j < 4; j++)
		{
			sum += arr[i][j];
		}
		cout << "第i行的元素之和为:" << sum << endl;
	}
	return 0;
}