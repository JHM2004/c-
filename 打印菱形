#include<iostream>
using namespace std;
int main()
{
	//打印菱形:
	int i = 0;
	int j = 0;
	for (i = 0; i < 5; i++)
	{
		for (j = 0; j < 5 - i; j++)
		{
			cout <<"  ";
		}
		for (j = 0; j < 2*(i + 1)-1; j++)
		{
			cout << "* ";
		}
		cout << endl;
	}	

	for (i = 0; i < 5; i++)
	{
		cout << "  ";
		for (j = 0; j <i+1; j++)
		{
			cout << "  ";
		}
		for (j = 0; j < 2*(5-i-1)-1; j++)
		{
			cout << "* ";
		}
		cout << endl;
	}
	return 0;
}